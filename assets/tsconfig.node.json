{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
        "$lib": ["svelte"],
        "$lib/*": ["svelte/*"]
    }
  },
  "include": ["vite.config.mts", "svelte.config.js"]
}
